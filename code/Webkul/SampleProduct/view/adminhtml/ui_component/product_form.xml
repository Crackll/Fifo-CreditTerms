        <block name="sampleproduct.form.sample.block" class="Webkul\SampleProduct\Block\Adminhtml\Product\Edit\Tab\Sample" template="Webkul_SampleProduct::catalog/product/edit/sample.phtml" as="sampleproduct_sample" ifconfig="sampleproduct/settings/enable"/>
    </htmlContent>
