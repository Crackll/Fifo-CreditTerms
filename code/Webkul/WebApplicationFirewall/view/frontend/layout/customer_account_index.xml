<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Webkul_WebApplicationFirewall::css/2fa.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Webkul\WebApplicationFirewall\Block\Account\Dashboard\TwoStepAuth" name="customer_account_two_step_authentication" as="two_step_auth" after="info" template="Webkul_WebApplicationFirewall::account/dashboard/two_step_auth.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
