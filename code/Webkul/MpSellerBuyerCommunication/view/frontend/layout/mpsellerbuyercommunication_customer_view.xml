<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Webkul_MpSellerBuyerCommunication::css/mpsellerbuyercomm.css"/>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Seller Communication History</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Webkul\MpSellerBuyerCommunication\Block\Customer\View" name="mpsellerbuyercommunication_customer_view" template="customer/view.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
